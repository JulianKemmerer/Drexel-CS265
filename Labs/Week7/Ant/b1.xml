<project default="hello">
	<description>
		A simple project, a couple targets, and just 'echo' tasks.
		Note that this description is optional.
	</description>

  <target name="hello">
    <echo message="Hello, World"/>
  </target>

  <target name="bye">
    <echo message="Goodbye, Cruel World"/>
  </target>

  <target name="all" depends="hello,bye">
		<echo message="Done!"/>
	</target>
</project>

